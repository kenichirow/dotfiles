[core]
	editor = vim
  excludesfile = ~/.gitignore
	quotepath = false
  cleanup = whitespace
[color]
	ui = auto
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[user]
	name = kenichirow
	email = allagesxxx@gmail.com
[github]
	user = kenichirow

[alias]
  l  = log
  ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
  ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
  s  = status -s
  b  = branch -a
  co = checkout
  po = pull origin
  pull-update = "!sh -c 'git pull origin develop` git submodule update -i`'"
[push]
	default = simple
[commit]
	cleanup = whitespace
